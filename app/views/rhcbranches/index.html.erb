<h1>Listing rhcbranches</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rhcbranches.each do |rhcbranch| %>
      <tr>
        <td><%= rhcbranch.branch %></td>
        <td><%= link_to 'Show', rhcbranch %></td>
        <td><%= link_to 'Edit', edit_rhcbranch_path(rhcbranch) %></td>
        <td><%= link_to 'Destroy', rhcbranch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rhcbranch', new_rhcbranch_path %>
