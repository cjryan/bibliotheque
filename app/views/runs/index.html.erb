<h1>Listing runs</h1>

<table>
  <thead>
    <tr>
      <th>Broker</th>
      <th>Testrun</th>
      <th>Caseruns</th>
      <th>Accounts</th>
      <th>Maxgears</th>
      <th>Tcms user</th>
      <th>Tcms password</th>
      <th>Rhcbranch</th>
      <th>Brokertype</th>
      <th>Accounts per job</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @runs.each do |run| %>
      <tr>
        <td><%= run.broker %></td>
        <td><%= run.testrun %></td>
        <td><%= run.caseruns %></td>
        <td><%= run.accounts %></td>
        <td><%= run.maxgears %></td>
        <td><%= run.tcms_user %></td>
        <td><%= run.tcms_password %></td>
        <td><%= run.rhcbranch %></td>
        <td><%= run.brokertype %></td>
        <td><%= run.accounts_per_job %></td>
        <td><%= link_to 'Show', run %></td>
        <td><%= link_to 'Edit', edit_run_path(run) %></td>
        <td><%= link_to 'Destroy', run, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Run', new_run_path %>
